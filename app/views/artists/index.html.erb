<%= render partial: "nav" %>
<br>
<div class="row mx-2">
  <% @artists.each_with_index do |artist,index| %>
    <% if(index % 4) == 0 %>
      <div class="clearfix"></div>
      <div class="col-sm-3 col-md-3 image-container">
        <%= image_tag artist.profile_picture_url, class: "detailimage" %>
        <p class="artistnamelink">
          <u><%= link_to "#{artist.name}", artist_path(artist) %></u>
        </p>
      </div>
    <% else %>
      <div class="col-sm-3 col-md-3 image-container">
        <%= link_to image_tag artist.profile_picture_url, class: "detailimage" %>
        <p class="artistnamelink">
        <u><%= link_to "#{artist.name}", artist_path(artist) %></u>
        </p>
      </div>
    <% end %>
  <% end %>
</div>
